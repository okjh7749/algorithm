   "source": [
    "T= int(input())\n",
    "cnt=0\n",
    "n=0\n",
    "for _ in range(T):\n",
    "    n =int(input())\n",
    "    dfs(0)\n",
    "    print(cnt)\n",
    "    cnt=0\n",
    "    \n",
    "def dfs(v):\n",
    "    global cnt\n",
    "    global n\n",
    "    if v > n:\n",
    "        return False\n",
    "    if v == n:\n",
    "        cnt=cnt+1\n",
    "    dfs(v+1)\n",
    "    dfs(v+2)\n",
    "    dfs(v+3)"
   ]
